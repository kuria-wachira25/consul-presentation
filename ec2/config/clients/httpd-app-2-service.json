{
  "service": {
    "name": "Httpd-App",
    "id": "httpd-app-2",
    "tags": ["v2.0"],
    "port": 80,
    "checks": [
      {
        "id": "Httpd App Http Check",
        "http": "http://localhost",
        "method": "GET",
        "interval": "10s",
        "timeout": "5s"
      },
      {
        "id": "Httpd App TCP Check",
        "tcp": "localhost:80",
        "interval": "10s",
        "timeout": "5s"
      }
    ]
  }
}
