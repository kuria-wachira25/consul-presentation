{
  "node_name": "NODE_NAME",
  "service": {
    "id": "SERVICE_ID",
    "name": "front-end-eCommerce",
    "tags": ["v7.05", "production"],
    "retry_join": [
      "provider=aws tag_key=Consul-Cluster-Name tag_value=demo-datacenter region=us-east-1"
    ],
    "encrypt": "ENCRYPTION_KEY",
    "encrypt_verify_incoming": true,
    "encrypt_verify_outgoing": true,
    "port": 80,
    "connect": {
      "sidecar_service": {}
    },
    "check": [
      {
        "id": "tcp-check",
        "name": "TCP Check PORT 80",
        "tcp": "localhost:80",
        "interval": "10s",
        "timeout": "1s"
      }
    ]
  }
}
